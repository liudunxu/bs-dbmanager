<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp_ID">
	<display-name>DataBaseManager</display-name>
	<servlet>
		<servlet-name>SqlQueryServlet</servlet-name>
		<display-name>SqlQueryServlet</display-name>
		<description></description>
		<servlet-class>com.data.manager.servlet.SqlQueryServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ExcelExportServlet</servlet-name>
		<display-name>ExcelExportServlet</display-name>
		<description></description>
		<servlet-class>com.data.manager.servlet.ExcelExportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SqlQueryServlet</servlet-name>
		<url-pattern>/SqlQueryServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ExcelExportServlet</servlet-name>
		<url-pattern>/ExcelExportServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
